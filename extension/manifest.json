{
  "manifest_version": 2,
  "name": "Chromium Bug Issue 766433",
  "description": "This extension is an example for Chromium Bug Issue 766433",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "background": {
    "scripts": [
        "src/background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "*://*/*"
  ]
}
